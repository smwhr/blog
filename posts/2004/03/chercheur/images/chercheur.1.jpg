                        }

                        $(this).parents('#responsive-menu-pro').find('a.responsive-menu-pro-item-link').filter(':visible').first().focus();
                        break;
                    case 37:
                        event.preventDefault();
                        event.stopPropagation();

                                                if ( link.parent('li').prevAll('li').filter(':visible').first().length == 0) {
